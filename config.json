{
    "user": {
        "extensions": {
            "Afbeeldingen": [".jpg", ".jpeg", ".png", ".gif", ".bmp", ".tiff", ".webp"],
            "Documenten": [".doc", ".docx", ".pdf", ".txt", ".odt", ".rtf", ".xls", ".xlsx", ".ppt", ".pptx"],
            "Muziek": [".mp3", ".wav", ".flac", ".m4a", ".aac"],
            "Video's": [".mp4", ".avi", ".mov", ".wmv", ".mkv"],
            "Adobe": [".psd", ".indd", ".ai", ".ait", ".idml", ".inx"],
            "Backups": [".dbk", ".bak", ".zip", ".7z", ".rar", ".tar", ".gz", ".iso", ".dbb"],
            "Boeken": [".epub", ".mobi", ".azw3", ".cbr", ".cbz"]
        },
        "directories": [
            "documents", "downloads", "pictures", "photos", "music", "videos", "desktop", "boeken", "documenten", "afbeeldingen"
        ]
    },
    "system": {
        "extensions": {
            "Uitvoerbare Bestanden": [".exe", ".msi", ".com", ".scr", ".pif", ".appx", ".msix"],
            "Scripts": [".bat", ".cmd", ".vbs", ".ps1"],
            "Systeembestanden": [".dll", ".sys", ".drv", ".ocx", ".cpl"],
            "Configuratie": [".ini", ".reg", ".inf", ".pol", ".adm", ".admx"],
            "Documentatie": [".hlp", ".chm"],
            "Logboeken en Data": [".log", ".tmp", ".dat", ".etl", ".evtx", ".dmp", ".mdmp", ".pdb"],
            "Add-ons": [".theme", ".deskthemepack", ".gadget"],
            "Iconen": [".ico"],
            "Windows Specifiek": [".wim", ".esd", ".mui", ".nls", ".cab", ".msp", ".msu"],
            "Snelkoppelingen": [".lnk", ".url", ".library-ms", ".search-ms", ".scf", ".job"],
            "Overig": [".manifest"]
        },
        "directories": [
            "windows", "program files", "programdata", "drivers", "system32", "intel", "nvidia"
        ],
        "files": [
            "setup", "autorun", "token", "config", "install", "background", "log", "temp", "patch", "uninstall", "driver", "license", "readme", "support", "windows"
        ]
    },
    "ignore": {
        "extensions": [
            ".ds_store", "thumbs.db", "desktop.ini", "icon\r", "autorun.inf"
        ],
        "directories": [
            "__macosx", "system volume information"
        ]
    }
}
